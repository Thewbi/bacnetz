<app:App title="BACnetz FatClient" maximized="true"
  xmlns:bxml="http://pivot.apache.org/bxml"
  xmlns:app="de.bacnetz.fatclient"
  xmlns="org.apache.pivot.wtk">
	<Border styles="{color:10}">
		<TablePane>
			<columns>
				<TablePane.Column width="1*" />
			</columns>
			<TablePane.Row>
				<PushButton bxml:id="btToogleDoors" buttonData="Toggle DoorOpen" />
			</TablePane.Row>
		</TablePane>
	</Border>
</app:App>